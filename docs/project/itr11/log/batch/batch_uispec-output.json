    "reactiveBindings": {
      "bindings": [
        {
          "id": "rb_diverge_to_organize_cards",
          "source": "brainstorm_cards_0.cards",
          "target": "card_sorting_0.cards",
          "mechanism": "update",
          "relationship": {
            "type": "passthrough"
          },
          "updateMode": "realtime"
        },
        {
          "id": "rb_organize_to_converge_categories_to_items",
          "source": "card_sorting_0.categories",
          "target": "priority_slider_grid_0.items",
          "mechanism": "update",
          "relationship": {
            "type": "javascript",
            "javascript": "Object.values(source).flat().map((catItem, idx) => ({ id: catItem.id || ('item_'+idx), label: catItem.text || catItem.idea || catItem.label || '無題', priority: 50 }))"
          },
          "updateMode": "debounced",
          "debounceMs": 300
        },
        {
          "id": "rb_priority_to_actionplan",
          "source": "priority_slider_grid_0.selectedItem",
          "target": "action_plan_creator_0.input",
          "mechanism": "update",
          "relationship": {
            "type": "javascript",
            "javascript": "if(!source) return []; const s = Array.isArray(source)? source: [source]; return s.map((it, i) => ({ selectedIdea: it.label||it.text||'無題', priorityRank: 1+i, first30DaysPlan: ['最初のタスクを定義する'], requiredResources: [], estimatedBreakEvenMonths: 3, nextAction: { task: 'まずは情報収集', due: '7日後', owner: '自分' } }));"
          },
          "updateMode": "debounced",
          "debounceMs": 300
        }
      ]
    },
